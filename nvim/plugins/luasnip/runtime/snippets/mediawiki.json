  "del": {
    "prefix": [
      "del",
      "дел"
    ],
    "body": "<del>${1:$TM_SELECTED_TEXT}</del>",
    "description": "Deleted text"
  },
  "code": {
    "prefix": [
      "code",
      "код"
    ],
    "body": "<code>${1:$TM_SELECTED_TEXT}</code>",
