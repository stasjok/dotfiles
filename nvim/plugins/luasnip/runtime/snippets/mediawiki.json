  },
  "h4": {
    "prefix": [
      "h4",
      "з4"
    ],
    "body": "==== ${1:$TM_SELECTED_TEXT} ====",
    "description": "Heading level 4"
  },
  "h5": {
    "prefix": [
      "h5",
      "з5"
    ],
    "body": "===== ${1:$TM_SELECTED_TEXT} =====",
    "description": "Heading level 5"
  },
  "h6": {
    "prefix": [
      "h6",
      "з6"
    ],
    "body": "====== ${1:$TM_SELECTED_TEXT} ======",
    "description": "Heading level 6"
  },
  "hl": {
    "prefix": [
      "hl",
      "хл"
    ],
    "body": "----",
    "description": "Horizontal rule"
  },
  "preformatted": {
    "prefix": "preformatted",
