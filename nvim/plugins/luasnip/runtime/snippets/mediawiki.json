  },
  "h2": {
    "prefix": [
      "h2",
      "з2"
    ],
    "body": "== ${1:$TM_SELECTED_TEXT} ==",
    "description": "Heading level 2"
  },
  "h3": {
    "prefix": [
      "h3",
      "з3"
    ],
