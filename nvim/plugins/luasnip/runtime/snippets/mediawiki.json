{
  "i": {
    "prefix": [
      "i",
      "и"
    ],
    "body": "''${1:$TM_SELECTED_TEXT}''",
    "description": "Italic text"
  },
  "b": {
    "prefix": [
      "b",
      "б"
    ],
    "body": "'''${1:$TM_SELECTED_TEXT}'''",
    "description": "Bold text"
  },
  "bi": {
    "prefix": [
      "bi",
      "би"
    ],
    "body": "'''''${1:$TM_SELECTED_TEXT}'''''",
    "description": "Bold and italic text"
  },
  "nowiki": {
    "prefix": [
      "nowiki",
      "ноувики"
    ],
    "body": "<nowiki>${1:$TM_SELECTED_TEXT}</nowiki>",
